   "source": [
    "def next_symptom(file, symptom_dict):\n",
    "    \"\"\"\n",
    "        Generator for label sets from the label file\n",
    "    \"\"\"\n",
    "    reader = csv.reader(file)\n",
    "    #header\n",
    "    next(reader)\n",
    "\n",
    "    first_line = next(reader)\n",
    "    \n",
    "    cur_pcr_key = first_line[0]\n",
    "    cur_symptom = [symptom_dict[first_line[1]]]\n",
    "\n",
    "    for row in reader:\n",
    "        pcr_key = row[0]\n",
    "        symptom = symptom_dict[row[1]]\n",
    "\n",
    "        #keep reading until you hit a new hadm id\n",
    "        if pcr_key != cur_pcr_key:\n",
    "            yield cur_pcr_key, cur_symptom\n",
    "            cur_pcr_key = pcr_key\n",
    "            cur_symptom = [symptom]\n",
    "        else:\n",
    "            #add to the labels and move on\n",
    "            cur_symptom.append(symptom)\n",
    "\n",
    "    yield cur_pcr_key, cur_symptom"
   ]
  },
