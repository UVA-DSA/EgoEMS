    "def next_patient(file):\n",
    "    \"\"\"\n",
    "        Generator for label sets from the label file\n",
    "    \"\"\"\n",
    "    reader = csv.reader(file)\n",
    "    #header\n",
    "    next(reader)\n",
    "\n",
    "    first_line = next(reader)\n",
    "    \n",
    "    cur_pcr_key = first_line[0]\n",
    "    cur_chief_complaint = [NEMSIS_chief_complaints_dict[first_line[1]]]\n",
    "    cur_gender = [gender_dict[first_line[2]]]\n",
    "    \n",
    "    cur_age_converted = age_dict[first_line[3]] if first_line[3] in age_dict else first_line[3]\n",
    "    cur_age = [cur_age_converted]\n",
    "    \n",
    "    cur_age_unit = [age_unit_dict[first_line[4]]]\n",
    "\n",
    "    for row in reader:\n",
    "        pcr_key = row[0]\n",
    "        chief_complaint = NEMSIS_chief_complaints_dict[row[1]]\n",
    "        gender = gender_dict[row[2]]\n",
    "        age = age_dict[row[3]] if row[3] in age_dict else row[3]\n",
    "        age_unit = age_unit_dict[row[4]]\n",
    "\n",
    "        #keep reading until you hit a new hadm id\n",
    "        if pcr_key != cur_pcr_key:\n",
    "            yield cur_pcr_key, cur_chief_complaint, cur_gender, cur_age, cur_age_unit\n",
    "            cur_pcr_key = pcr_key\n",
    "            cur_chief_complaint = [chief_complaint]\n",
    "            cur_gender = [gender]\n",
    "            cur_age = [age]\n",
    "            cur_age_unit = [age_unit]\n",
    "        else:\n",
    "            #add to the labels and move on\n",
    "            cur_chief_complaint.append(chief_complaint)\n",
    "            cur_gender.append(gender)\n",
    "            cur_age.append(age)\n",
    "            cur_age_unit.append(age_unit)\n",
    "    yield cur_pcr_key, cur_chief_complaint, cur_gender, cur_age, cur_age_unit"
   ]
  },
