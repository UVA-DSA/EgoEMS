    "def next_med(file):\n",
    "    \"\"\"\n",
    "        Generator for label sets from the label file\n",
    "    \"\"\"\n",
    "    reader = csv.reader(file)\n",
    "    #header\n",
    "    next(reader)\n",
    "\n",
    "    first_line = next(reader)\n",
    "    \n",
    "    cur_pcr_key = first_line[0]\n",
    "    cur_med_03 = [NEMSIS_med_dict[first_line[1]]]\n",
    "    \n",
    "    for row in reader:\n",
    "        pcr_key = row[0]\n",
    "        med_03 = NEMSIS_med_dict[row[1]]\n",
    "\n",
    "        #keep reading until you hit a new hadm id\n",
    "        if pcr_key != cur_pcr_key:\n",
    "            yield cur_pcr_key, cur_med_03\n",
    "            cur_pcr_key = pcr_key\n",
    "            cur_med_03 = [med_03]\n",
    "        else:\n",
    "            #add to the labels and move on\n",
    "            if med_03 not in cur_med_03:\n",
    "                cur_med_03.append(med_03)\n",
    "            \n",
    "    yield cur_pcr_key, cur_med_03"
   ]
